[style.css]==========================================================

html,body{padding:0;margin:0;background:#dadada;font-size:1.05rem;font-family:arial}
.container{display:grid;grid-template-columns:20% auto 20%;}
.container .body,.container .menu,.container .footer{grid-column-start:2;grid-column-end:3;}
.container .menu ul{padding:0;margin:0;background:#000;padding:.5rem}
.container .menu ul li{list-style:none;display:inline-block;}
.container .menu ul li a{text-decoration:none;display:block;padding:.5rem 1rem;background:#febc11;color:#000;border-radius:3px;transition:all .2s;}
.container .menu ul li a.active{background:#fff;}
.container .menu ul li a:hover{opacity:.8;}
.container .body{min-height:60vh;background:#fff;padding:2rem}
.container .footer{padding:1rem;background:#febc11;color:#000;text-align:center;}
.card img{max-width:200px;border-radius:15px;margin-right:15px;float: left;;}
.card{display:table;width:calc(100% - 2rem);border-radius:10px;border:solid 1px #dadada;padding:1rem;}
.container .menu{padding-left:200px;background:#000;position:relative;}
